{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "linebreak-style": ["error", "windows"]
  },
  "extends": ["eslint:recommended", "google"]
}
